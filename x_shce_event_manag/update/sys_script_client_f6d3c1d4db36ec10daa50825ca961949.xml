        <description>Shows/Hides the food service section of fields based on the value of the checkbox</description>
        <field>food_service</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Show/Hide Food Function Fields</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading || newValue === '') {
        return;
    }
	
    //Type appropriate comment here, and begin script below
	if (newValue == 'true') {
		g_form.setSectionDisplay('food_servicesrequested', true);
	} else {
		g_form.setSectionDisplay('food_servicesrequested', false);
	}
	
	g_tabs2Sections.setActive(0);
}]]></script>
