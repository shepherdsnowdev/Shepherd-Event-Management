    <sys_script_client action="INSERT_OR_UPDATE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>Shows/Hides the room reservation section of fields based on the value of the checkbox</description>
        <field>room_reservation</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Show/Hide Room Reservation Fields</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading || newValue === '') {
        return;
    }
	
    //Type appropriate comment here, and begin script below
	if (newValue == 'true') {
		g_form.setSectionDisplay('room_reservation', true);
	} else {
		g_form.setSectionDisplay('room_reservation', false);
	}
	
	g_tabs2Sections.setActive(0);
}]]></script>
