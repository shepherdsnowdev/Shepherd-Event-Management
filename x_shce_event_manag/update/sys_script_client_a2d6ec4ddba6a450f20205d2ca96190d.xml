        <name>Hide Room/Food sections on load</name>
        <order/>
        <script><![CDATA[function onLoad() {
    if (g_form.getValue('room_reservation') == 'false') {
		g_form.setSectionDisplay('room_reservation', false);
	} else {
		g_form.setSectionDisplay('room_reservation', true);
	}
	
	if (g_form.getValue('food_service') == 'false') {
		g_form.setSectionDisplay('food_servicesrequested', false);
	} else {
		g_form.setSectionDisplay('food_servicesrequested', true);
	}
		
	g_tabs2Sections.setActive(0);
}]]></script>
