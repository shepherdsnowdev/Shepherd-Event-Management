    <sys_script_client action="DELETE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>When "Function Location" gets changed to "Other", show the "Function Location - Other" field and blank it's previous value</description>
        <field>function_location</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Function Location Other - show/hide</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading || newValue === '') {
        return;
    }

    //Type appropriate comment here, and begin script below
    if (newValue == 'Other') {
        g_form.setValue('function_location_other', '');
        g_form.setDisplay('function_location_other', 'True');
		g_form.setMandatory('function_location_other', 'True');
    } else {
		g_form.setValue('function_location_other', '');
        g_form.setDisplay('function_location_other', 'False');
		g_form.setMandatory('function_location_other', 'False');
	}
}]]></script>
